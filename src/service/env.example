# Credential Manager Configuration
# Choose one of: localStorage, masca, infuraIPFS, serverWeb3Storage
VITE_CREDENTIAL_MANAGER_TYPE=serverWeb3Storage

# Infura IPFS Configuration (required when using infuraIPFS)
VITE_INFURA_IPFS_PROJECT_ID=your_infura_project_id_here
VITE_INFURA_IPFS_PROJECT_SECRET=your_infura_project_secret_here
VITE_INFURA_IPFS_ENDPOINT=https://ipfs.infura.io:5001

# Server Web3.Storage Configuration (required when using serverWeb3Storage)
# These should be set in your server's .env file, not the client
WEB3_STORAGE_EMAIL=your_email@example.com
WEB3_STORAGE_SPACE_DID=did:key:your_space_did_here

# Debug mode for IPFS operations (optional)
VITE_DEBUG_IPFS=false
VITE_DEBUG_WEB3STORAGE=false 